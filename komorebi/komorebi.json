  "border_colours": {
    "single": {
      "r": 0,
      "g": 120,
      "b": 200,
    },
    "stack": {
      "r": 0,
      "g": 165,
      "b": 66
    },
    "monocle": {
      "r": 255,
      "g": 51,
      "b": 153
    },
    "unfocused": {
      "r": 128,
      "g": 128,
      "b": 128
    }
  },
  "border_style": "System",
  "border_z_order": "Bottom",
  "border_implementation": "Windows",
  "transparency": false,
  "transparency_alpha": 220,
  "monitors": [

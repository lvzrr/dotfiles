{
  "default_workspace_padding": 0,
  "default_container_padding": 10,
  "border_width": 25,
  "border_offset": -1,
    "global_work_area_offset":{
        "top": 0,
        "bottom": 0,
        "left":0,
        "right":0,
    },
  "minimum_window_width": 0,
  "minimum_window_height": 0,
  "resize_delta": 50,
  "window_container_behaviour": "Create",
  "cross_monitor_move_behaviour": "Swap",
